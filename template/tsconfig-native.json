{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",

    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ]
  },

  "exclude": [
    "../*/web/**/*",
    "../*/jspm_packages/**/*",
    "../*/node_modules/**/*"
  ]

}
