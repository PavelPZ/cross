{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "inlineSourceMap": true,
    "inlineSources": true,
    "importHelpers": true,

    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "paths": {
      "rw-utils/*": [ "d:/rw/lib/rw-utils/*" ],
      "rw-router/*": [ "d:/rw/lib/rw-router/*" ]
    }
  },

  "include": [
    "index.tsx"
  ],

  "exclude": [
    "../*/jspm_packages/**/*",
    "../*/node_modules/**/*"
  ]

}
